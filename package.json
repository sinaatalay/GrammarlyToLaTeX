{
  "name": "grammarlytolatex",
  "version": "1.0.0",
  "description": "A browser extension to convert Grammarly text to LaTeX.",
  "scripts": {
    "format": "prettier --write .",
    "audit": "npm audit --production",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinaatalay/GrammarlyToLaTeX.git"
  },
  "keywords": [
    "Grammarly",
    "LaTeX"
  ],
  "author": "Sina Atalay",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sinaatalay/GrammarlyToLaTeX/issues"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "turndown": "^7.1.1"
  },
  "devDependencies": {
    "prettier": "^2.8.3",
    "react-scripts": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
